<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gary.mapper.UserAuthorityMapper">

    <!-- query user's authority -->
    <select id="findByid" parameterType="Integer" resultType="com.gary.model.UserAuthorityEntity">
        select * from sys_authority where id = #{value};
    </select>

</mapper>